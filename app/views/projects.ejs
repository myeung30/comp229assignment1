<%- include ('partials/header.ejs') %>
<%- include ('partials/nav.ejs') %>

<h1><%= title %></h1>
<div class="row row-cols-3 row-cols-md-3 g-2">
  <div class="col">
    <div class="card h-100">
      <img src="img/todo.jpg" class="card-img-top" alt="todo">
      <div class="card-body">
        <h5 class="card-title">To-do list</h5>
        <p class="card-text">This is a reminder for users to create a to-do checklist. Users can add schedule into it, cross it when completed and delete it from the list.</p>
        <a href="documents/project1/todo.html" class="btn btn-primary stretched-link">To-do List</a>
      </div>
    </div>
  </div>
  <div class="col">
    <div class="card h-100">
      <img src="img/wordguess.png" class="card-img-top" alt="wordguess">
      <div class="card-body">
        <h5 class="card-title">Word Guessing Game</h5>
        <p class="card-text">Guess the word as fast as you can. </p>
        <a href="documents/project2/wordguess.html" class="btn btn-primary stretched-link">Word Guessing Game</a>
      </div>
    </div>
  </div>
  <div class="col">
    <div class="card h-100">
      <img src="img/catapi.png" class="card-img-top" alt="catapi">
      <div class="card-body">
        <h5 class="card-title">Cat API</h5>
        <p class="card-text">Using the cat API, random cat picture can be generated by some features</p>
        <a href="documents/project3/catapi.html" class="btn btn-primary stretched-link">Cat API</a>
      </div>
    </div>
  </div>
</div>

